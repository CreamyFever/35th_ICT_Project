<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
<!-- typeAlias -->	
	
	<typeAliases>
		<typeAlias type="com.creamyfever.wow.vo.Members" alias="Members" />
		<typeAlias type="com.creamyfever.wow.vo.Article" alias="Article" />
		<typeAlias type="com.creamyfever.wow.vo.ArticleHtml" alias="ArticleHtml" />
		<typeAlias type="com.creamyfever.wow.vo.Board" alias="Board" />
		<typeAlias type="com.creamyfever.wow.vo.Reply" alias="Reply" />
 	    <typeAlias type="com.creamyfever.wow.vo.Topkeyword" alias="Topkeyword" />
 	    <typeAlias type="com.creamyfever.wow.vo.Discussion" alias="Discussion" />
 	    <typeAlias type="com.creamyfever.wow.vo.Discussion_log" alias="Discussion_log" />
 	    <typeAlias type="com.creamyfever.wow.vo.Discussion_member" alias="Discussion_member" />
 	    <typeAlias type="com.creamyfever.wow.vo.Discussion_rooms" alias="Discussion_rooms" />
		<typeAlias type="com.creamyfever.wow.vo.Keyword" alias="Keyword"/>
 	    <typeAlias type="com.creamyfever.wow.vo.AgeGender" alias="AgeGender" />
	</typeAliases>
		

<!-- mapper -->
	
	<mappers>
		<mapper resource="mappers/MembersMapper.xml" />
		<mapper resource="mappers/ArticleMapper.xml" />
 		<mapper resource="mappers/BoardMapper.xml" />
 		<mapper resource="mappers/WorldmapMapper.xml" />
 		<mapper resource="mappers/DiscussionMapper.xml" />
 		<mapper resource="mappers/KeywordMapper.xml" />
 		<mapper resource="mappers/GraphMapper.xml" />
	</mappers>	

</configuration>